<view>
  <swiper indicator-dots="{{indicatorDots}}" indicator-color="#fff" indicator-active-color="red"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}" wx:key="index">
      <swiper-item>
        <image src="{{item.imagesUrl}}" class="slide-image" data-id="{{item.id}}" data-title="{{item.title}}" />
      </swiper-item>
    </block>
  </swiper>
